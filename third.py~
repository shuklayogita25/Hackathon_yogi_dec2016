__author__ = 'Yogita Shukla'
def step(s , e, count):
    if s > e:
        s = s-1
        count = count + 1
        step(s = s, e = e, count = count)

    elif s < e:
        s = s * 2
        count = count + 1
        step(s = s, e = e, count = count)

def main():
    data = int(input("Enter a number: "))
    
    print (data)


if __name__ =='__main__':
    main()